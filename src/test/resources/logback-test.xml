<configuration>

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d [%highlight(%-5level)] %logger{5} - %msg%n</pattern>
    </encoder>
  </appender>

  <logger name="com.github.ybr.zk" level="TRACE" />

  <root level="ERROR">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>
